    "def bub_sort(codr):\n",
    "    for i in range(len(codr)):\n",
    "        for j in range (len(codr)-i-1):\n",
    "            if codr[j]<codr[j+1]:\n",
    "                n = codr[j+1]\n",
    "                codr[j+1] = codr[j]\n",
    "                codr[j] = n\n",
    "    return codr\n",
    "bub_sort([1,1,9,9,2,22,3,4,6,])"
   ]
